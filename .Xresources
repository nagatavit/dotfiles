! Transparency (0-1):
St.alpha: 0.9

! Dracula color scheme
St.foreground: #F8F8F2
St.background: #282A36
St.color0:     #262626
St.color1:     #E356A7
St.color2:     #42E66C
St.color3:     #E4F35A
St.color4:     #9B6BDF
St.color5:     #E64747
St.color6:     #75D7EC
St.color7:     #EFA554
St.color8:     #7A7A7A
St.color9:     #FF79C6
St.color10:    #50FA7B
St.color11:    #F1FA8C
St.color12:    #BD93F9
St.color13:    #FF5555
St.color14:    #8BE9FD
St.color15:    #FFB86C

St.font: monospace:size=11

URxvt.intensityStyles:	false
URxvt.depth:		32
URxvt.font: 		xft:FiraCode:size=11, xft:MesloLGS NF:size=11:style=Bold, xft:PowerlineSymbols
URxvt.scrollBar:	false
URxvt.cursorColor:	white

!! Extensions
URxvt.perl-ext-common:  default,matcher,resize-font,url-select,keyboard-select,tabbedex,fullscreen
URxvt.colorUL:              #4682B4
!! Fullscreen
URxvt.keysym.F11: perl:fullscreen:switch
!! url-select
URxvt.keysym.M-u:   perl:url-select:select_next
URxvt.url-select.launcher: linkhandler
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keyboard-select.clipboard: true
!! resize-font
URxvt.keysym.C-Down:  resize-font:smaller
URxvt.keysym.C-Up:    resize-font:bigger
URxvt.iso14755: false
URxvt.iso14755_52: false
!! Matcher
URxvt.url-launcher: linkhandler
URxvt.matcher.button:   1
