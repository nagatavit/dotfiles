set hlsearch
hi Search guibg=peru guifg=wheat
hi Search ctermbg=LightYellow
hi Search ctermfg=Black
set number
set tabstop=4
set autoindent
imap <Tab> <C-X><C-F>
set is
set ic
syntax on
set smartindent
set ts=4
set sw=4
map <C-h> :tabprev
map <C-l> :tabnext
map <C-j> <PageDown>
map <C-k> <PageUp>
set pastetoggle=<F12>
